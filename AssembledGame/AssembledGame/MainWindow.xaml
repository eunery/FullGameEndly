<Window x:Name="Level1"
    x:Class="AssembledGame.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:AssembledGame"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <TextBlock x:Name="TimerBlockDown"
                   HorizontalAlignment="Center"
                   VerticalAlignment="Top"
                   FontSize="35">
        </TextBlock>
        <Button x:Name="Button"
                Content="Не кликай, иначе проиграешь"
                HorizontalAlignment="Center"
                VerticalAlignment="Center"/>
        <Button x:Name="ChooseLevel"
                Content="Выбрать уровень." 
                HorizontalAlignment="Left"
                VerticalAlignment="Bottom"/>
        <TextBox x:Name="tbChoose"
                 HorizontalAlignment="Center"
                 Height="23"
                 TextWrapping="Wrap"
                 VerticalAlignment="Bottom"
                 Width="120"/>
    </Grid>
</Window>
